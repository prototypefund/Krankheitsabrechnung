	@mkdir -p .tmp/testgm && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/testgm/ && $(COPY_FILE) --parents form1.ui.h .tmp/testgm/ && ( cd `dirname .tmp/testgm` && $(TAR) testgm.tar testgm && $(GZIP) testgm.tar ) && $(MOVE) `dirname .tmp/testgm`/testgm.tar.gz . && $(DEL_FILE) -r .tmp/testgm

